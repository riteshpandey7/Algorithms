//method1 : using main class:
package method;
import java.util.Scanner;

public class Rough2
{
public static void main(String[] args)
	{
		Scanner scn= new Scanner(System.in);
		int a = scn.nextInt();
		
		int[] arr= new int[a];
		
		for (int i=0; i<arr.length; i++)
		{
			arr[i]=scn.nextInt();
			
		}
		int sum=0;
		for (int i=0; i<arr.length; i++)
		{
			
			sum= sum+arr[i];
		}
		System.out.println(sum);
	
}

//method1 : using main class:
package method;
import java.util.Scanner;

public class Rough2
{
public static void main(String[] args)
	{
		Scanner scn= new Scanner(System.in);
		int a = scn.nextInt();
		
		int[] arr= new int[a];
		
		for (int i=0; i<arr.length; i++)
		{
			arr[i]=scn.nextInt();
			
		}
		int sum=0;
		for (int i=0; i<arr.length; i++)
		{
			
			sum= sum+arr[i];
		}
		System.out.println(sum);
	
}

//method 2: using method+ for loop
package method;
import java.util.Scanner;

public class Rough2
{
public static void main(String[] args)
	{
		Scanner scn= new Scanner(System.in);
		int a = scn.nextInt();
		
		int[] arr= new int[a];
		
		for (int i=0; i<arr.length; i++)
		{
			arr[i]=scn.nextInt();
			
		}
		
		System.out.println(Rough2.summ(arr));
	}
	
	
	public static int summ(int[] arr)
	{
		int sum=0;
		for(int i=0; i<arr.length; i++)
		{
			sum+=arr[i];
		}
		return sum;
	}
}


